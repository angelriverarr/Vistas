@model IEnumerable<TiendaOnline.Models.Carritos>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
     Layout = "~/Views/Shared/_LayoutCarrito.cshtml";
}
<div class="container pedido-carrito">
    <table class="table">
  <thead>
    <tr>
      <th>Id</th>
       <th>Cantidad</th>
        <th>Nombre</th>
       <th>Descripcion</th>
      <th>Precio</th>
    </tr>
  </thead>
  <tbody>
            @foreach (var item in ViewBag.joinn)
            {
                <tr>
                    <td>@item.artId</td>
                    <td>@item.cantida</td>
                    
                    <td>@item.nombre</td>
                    <td>@item.descripcion</td>
                    <td>@item.precio </td>
                </tr>
                
            }
        </tbody>
</table>
</div>